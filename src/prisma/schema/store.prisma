model Store {
   id       String   @id @default(uuid())
  name      String    @unique
   products  Product[]
  email     String?
  address   String?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?

   userId    String
  user      User      @relation(fields: [userId], references: [id])

 @@map("store")
}